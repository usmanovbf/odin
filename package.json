    "serverless-prune-versions": "^1.0.3",
    "serverless-stack-output": "^0.2.3",
    "serverless-webpack": "^5.4.1",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "webpack": "^5.31.2",
    "webpack-node-externals": "^2.5.2"
