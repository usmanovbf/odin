    "@babel/preset-typescript": "^7.13.0",
    "@serverless/event-mocks": "^1.1.1",
    "@types/aws-lambda": "^8.10.75",
    "@types/aws-sdk": "^2.7.0",
    "@types/chance": "^1.1.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "aws-sdk": "^2.884.0",
